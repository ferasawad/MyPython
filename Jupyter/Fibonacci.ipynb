    "#   this function is too slow\n",
    "def fibonacci_1(n):\n",
    "    if n == 1 :\n",
    "        return 1\n",
    "    elif n == 2 :\n",
    "        return 1\n",
    "    elif n > 2 :\n",
    "        return fibonacci_1(n-1) + fibonacci_1(n-2)"
   ]
  },
